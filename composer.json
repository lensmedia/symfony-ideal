{
    "name": "lensmedia/symfony-ideal",
    "description": "\"Private\" bundle for handling iDeal transactions",
    "license": "proprietary",
    "type": "symfony-bundle",
    "version": "2.0.5",
    "require": {
        "php": ">=8.2",
        "ext-openssl": "*",
        "brick/money": "*",
        "symfony/framework-bundle": "*",
        "symfony/http-client": "*",
        "symfony/monolog-bundle": "*",
        "symfony/uid": "*",
        "symfony/validator": "*",
        "symfony/serializer-pack": "*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "*"
    },
    "autoload": {
        "psr-4": {
            "Lens\\Bundle\\IdealBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
