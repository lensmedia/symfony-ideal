{
    "name": "lensmedia/symfony-ideal",
    "description": "\"Private\" bundle for handling iDeal transactions",
    "license": "proprietary",
    "type": "symfony-bundle",
    "version": "2.0",
    "require": {
        "php": ">=8.2",
        "ext-openssl": "*",
        "brick/money": "^0.8",
        "symfony/framework-bundle": "^6.0 || ^7.0",
        "symfony/http-client": "^6.0 || ^7.0",
        "symfony/monolog-bundle": "^3.10",
        "symfony/uid": "^6.0 || ^7.0",
        "symfony/validator": "^6.0 || ^7.0",
        "symfony/serializer-pack": "^1.3"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.40"
    },
    "autoload": {
        "psr-4": {
            "Lens\\Bundle\\IdealBundle\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
